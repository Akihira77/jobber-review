{
  "name": "jobber-review",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start:dev": "set NODE_ENV=development&&nodemon -r tsconfig-paths/register src/app.ts | pino-pretty -c",
    "start:prod": "set NODE_ENV=production&&nodemon -r tsconfig-paths/register src/app.ts | pino-pretty -c",
    "start": "set NODE_ENV=production&&pm2 start ./build/src/app.js -i 5 --name Review_Service --attach --watch --trace | pino-pretty -c",
    "dev": "set NODE_ENV=production&&npm run build&&node --trace-warnings -r newrelic ./build/src/app.js",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "jest --coverage=false --maxWorkers=1 --forceExit --detectOpenHandles",
    "lint:check": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "prettier:check": "prettier --check ./src/**/*.{ts,json}",
    "prettier:fix": "prettier --write ./src/**/*.{ts,json}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@Akihira77/jobber-shared": "^0.0.17",
    "@elastic/elasticsearch": "^8.11.0",
    "@hono/node-server": "^1.11.2",
    "@types/node": "^20.14.0",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "hono": "^4.4.2",
    "hono-rate-limiter": "^0.3.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "newrelic": "^11.18.0",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "pino-pretty": "^10.3.1",
    "typescript": "^5.3.3",
    "typescript-transform-paths": "^3.4.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/amqplib": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0"
  },
  "description": ""
}
